{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.*",
        "patricktalmadge/bootstrapper": "4.1.*",
        "patrickrose/markdown-extender": "~1",
        "laracasts/presenter": "0.1.*",
        "symfony/dom-crawler" : "*,!=2.4.5"
    },
    "require-dev": {
        "way/generators": "~2",
        "barryvdh/laravel-ide-helper": "1.*",
        "behat/behat": "~2",
        "behat/mink": "~2",
        "behat/mink-extension": "~2",
        "behat/mink-goutte-driver": "~2",
        "behat/mink-selenium2-driver": "1.1.*",
        "fzaninotto/faker": "1.3.0",
        "patrickrose/whoops-configurator": "~1"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ],
        "psr-4": {
            "PatrickRose\\": "app/PatrickRose"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
